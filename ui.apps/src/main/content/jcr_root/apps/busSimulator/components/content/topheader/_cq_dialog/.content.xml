<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Top Header"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://confluence.embitel.com/x/QQLVD">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <logo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="logo"
                                                fileNameParameter="./logoImage"
                                                fileReferenceParameter="./logoReferenceImageCardImage"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                name="./logoPath"
                                                title="Upload Image"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <logoUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Logo URL"
                                                name="./logoUrl"
                                                required="{Boolean}true"
                                                rootPath="/content"/>
                                            <headerLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Recommended character count is max of 30 including spaces"
                                                fieldLabel="Header Label"
                                                name="./headerLabel"
                                                required="{Boolean}true"/>
                                            <headerUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Recommended character count is max of 30 including spaces"
                                                fieldLabel="Header Url"
                                                name="./headerUrl"
                                                required="{Boolean}true"/>
                                            <linkLabel1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Recommended character count is max of 30 including spaces"
                                                fieldLabel="Link Label 1"
                                                name="./linkLabel1"
                                                required="{Boolean}true"/>
                                            <linkUrl1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Link URL 1"
                                                name="./linkUrl1"
                                                required="{Boolean}true"
                                                rootPath="/content"/>
                                            <linkLabel2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Recommended character count is max of 30 including spaces"
                                                fieldLabel="Link Label 2"
                                                name="./linkLabel2"
                                                required="{Boolean}true"/>
                                            <linkUrl2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Link URL 2"
                                                name="./linkUrl2"
                                                required="{Boolean}true"
                                                rootPath="/content"/>
                                            <linkLabel3
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Recommended character count is max of 30 including spaces"
                                                fieldLabel="Link Label 3"
                                                name="./linkLabel3"
                                                required="{Boolean}true"/>
                                            <linkUrl3
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Link URL 3"
                                                name="./linkUrl3"
                                                required="{Boolean}true"
                                                rootPath="/content"/>
                                            <openinanewtab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}false"
                                                fieldDescription="Open in a new tab.(This only works for content pages and external URLs)"
                                                name="./openinanewtab"
                                                text="Open in a new tab"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            showhidetabtargetvalue="thick,thin"/>
                    </general>
                    <tracking
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tracking Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <enabletracking
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}false"
                                                fieldDescription="Enable click tracking"
                                                name="./trackingEnableClickTracking"
                                                text="Enable click tracking"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <trackinglocation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Location"
                                                name="./trackingLocation"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </tracking>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
